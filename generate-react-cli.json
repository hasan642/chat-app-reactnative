{
    "usesTypeScript": true,
    "testLibrary": "Testing Library",
    "component": {
        "default": {
            "customTemplates": {
                "component": "templates/component/component.tsx",
                "style": "templates/component/styles.ts"
            },
            "path": "src/components",
            "withStyle": true,
            "withTest": false,
            "withStory": false,
            "withLazy": false
        },
        "screen": {
            "customTemplates": {
                "component": "templates/component/component.tsx",
                "style": "templates/component/styles.ts"
            },
            "path": "src/screens",
            "withLazy": false,
            "withStory": false,
            "withStyle": true,
            "withTest": false
        }
    },
    "usesCssModule": false,
    "cssPreprocessor": "ts"
}